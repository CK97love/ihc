{
  "name": "server",
  "version": "1.0.0",
  "description": "local server implementation for EMR",
  "main": "build/init.js",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.16.2",
    "mongodb": "^3.0.1",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0"
  },
  "scripts": {
    "build": "babel 'src' --out-dir 'build' --presets=es2015 --copy-files --ignore 'node_modules'",
    "disableprebuild": "rimraf build/node_modules",
    "disablepostbuild": "cd build && npm install",
    "make": "npm run build",
    "server": "node build/init.js",
    "start": "npm run make && npm run server",
    "db": "mongod --dbpath=/data",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC"
}
